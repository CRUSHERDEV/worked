{
  "name": "linked-all-v1",
  "version": "1.0.0",
  "private": true,
  "description": "A pan-African, multi-vertical digital ecosystem - Linked All v1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "turbo run dev --concurrency=15",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "prettier": "^3.1.0",
    "turbo": "^1.11.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.20.0"
}
